generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int     @id @default(autoincrement())
  nomeCompleto String
  cpf_cnpj     String  @unique
  email        String  @unique
  senha        String
  logista      Boolean @default(false)

  @@map("users")
}
